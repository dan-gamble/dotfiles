{
	"calls": {
		"mute_on_join": true
	},
	"expand_excerpt_lines": 10,
	"extend_comment_on_newline": false,
	"performance": {
		"show_in_status_bar": true
	},
	"preferred_line_length": 120,
	"assistant": {
		"default_model": {
			"provider": "zed.dev",
			"model": "claude-3-5-sonnet-latest"
		},
		"version": "2"
	},
	"auto_update": true,
	"cursor_blink": false,
	"relative_line_numbers": true,
	"scrollbar": {
		"show": "never"
	},
	"tab_bar": {
		"show": false,
		"show_nav_history_buttons": false
	},
	"toolbar": {
		"breadcrumbs": true,
		"quick_actions": false
	},
	"git": {
		"git_gutter": "tracked_files"
	},
	"hard_tabs": true,
	"project_panel": {
		"auto_fold_dirs": true,
		"button": false
	},
	"tab_size": 2,
	"vim_mode": true,
	"ui_font_size": 14,
	"buffer_font_size": 14,
	"theme": "Catppuccin Mocha",
	"centered_layout": {
		"left_padding": 0.1,
		"right_padding": 0.1
	},
	"show_inline_completions": false,
	"format_on_save": "on",
	"code_actions_on_format": {
		"source.fixAll.biome": true,
		"source.organizeImports.biome": true
	},
	"formatter": {
		"language_server": {
			"name": "biome"
		}
	},
	"lsp": {
		"biome": {
			"settings": {
				"require_config_file": true
			}
		}
	}
}
